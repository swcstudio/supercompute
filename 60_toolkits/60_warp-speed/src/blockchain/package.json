{
  "name": "@warp-speed/defi-protocol",
  "version": "1.0.0",
  "description": "Warp-Speed DeFi Protocol - Consciousness-aware blockchain value anchoring system targeting $145.76B+ annual ETD generation",
  "main": "hardhat.config.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test",
    "test:coverage": "hardhat coverage",
    "deploy:local": "hardhat run deploy.js --network localhost",
    "deploy:goerli": "hardhat run deploy.js --network goerli",
    "deploy:sepolia": "hardhat run deploy.js --network sepolia", 
    "deploy:mainnet": "hardhat run deploy.js --network mainnet",
    "deploy:polygon": "hardhat run deploy.js --network polygon",
    "deploy:arbitrum": "hardhat run deploy.js --network arbitrum",
    "deploy:optimism": "hardhat run deploy.js --network optimism",
    "deploy:bsc": "hardhat run deploy.js --network bsc",
    "deploy:avalanche": "hardhat run deploy.js --network avalanche",
    "verify:mainnet": "hardhat verify --network mainnet",
    "verify:goerli": "hardhat verify --network goerli",
    "verify:polygon": "hardhat verify --network polygon",
    "node": "hardhat node",
    "console": "hardhat console",
    "flatten": "hardhat flatten",
    "size": "hardhat size-contracts",
    "clean": "hardhat clean",
    "export": "hardhat export --export-all ./deployments/deployments.json",
    "format": "prettier --write 'src/**/*.sol' 'test/**/*.js' 'deploy.js' 'hardhat.config.js'",
    "lint": "solhint 'src/**/*.sol'",
    "lint:fix": "solhint 'src/**/*.sol' --fix",
    "start:local": "concurrently \"hardhat node\" \"sleep 5 && npm run deploy:local\"",
    "dashboard": "hardhat run scripts/dashboard.js",
    "monitor": "hardhat run scripts/monitor.js",
    "upgrade": "hardhat run scripts/upgrade.js"
  },
  "keywords": [
    "defi",
